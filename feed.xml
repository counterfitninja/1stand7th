    <entry>
        <title>Pancake night.</title>
        <author>
            <name>Dave Bird</name>
        </author>
        <link href="https://counterfitninja.github.io/1stand7th/pancake-night/"/>
        <id>https://counterfitninja.github.io/1stand7th/pancake-night/</id>
